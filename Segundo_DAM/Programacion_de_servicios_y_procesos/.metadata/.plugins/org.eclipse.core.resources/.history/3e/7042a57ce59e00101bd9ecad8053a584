package ejercicio_7;

import java.io.File;

public class Lanzador {
	public static void main(String[] args) {
		
		try {

		File directorio = new File(".//bin");
		ProcessBuilder pb = new ProcessBuilder("java", "ejercicio_7.Main");

		pb.directory(directorio);

		Process proceso = pb.start();
		}
	}

}
